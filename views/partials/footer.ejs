	<script type="text/javascript">
	
	// Allow numbers only in Phone field
	function isNumber(evt) {
	    evt = (evt) ? evt : window.event;
	    var charCode = (evt.which) ? evt.which : evt.keyCode;
	    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
	        return false;
	    }
	    return true;
	}
	
	// round to 2 decimal places
	function roundNumber(num) { 
       return +(Math.round(num + "e+2")  + "e-2");
    }
    
    // initialization google autocomplete
    function initAutocomplete() {
		autocomplete = new google.maps.places.Autocomplete(document.getElementById('autocomplete'), options);
	}
	
	// return first letters of name string
    function getFirstLetters(name) {
        let nameStr = name.split(' '),
            resultName="";
        for (let i = 0; i < nameStr.length; i++) {
             if (i<3){
                // take first character & convert to uppercase
                 nameStr[i] = nameStr[i].charAt(0).toUpperCase();
                 // consider at most first 3 characters
                 resultName+=nameStr[i];
             } else {
                 break;
             }
        }

        return resultName;
    }
    
    // scroll to specific section
    function sectionFinder(x){
    	console.log($(x));
    	// if #contact found in same page scroll there otherwise go to main page
    	if ($('body, html').find('#contact').length!=0) {
    		let el =document.getElementById(x.classList[1]);
    		el.scrollIntoView({'behavior':'smooth'});
    		$(x).addClass('active-custom');
    	} else {
    		window.location.href='<%= baseURL %>#contact';
    	}
	}
	
	$(function(){
		// bootstrap js
		$('.dropdown-toggle').dropdown();

		// initialize datepicker, maximum selectable date is set 1month & 1week back from current date
		 $( "#datepicker" ).datepicker({
			maxDate: "-1m -1w"
	    });
		  
		// clicking on <tr> takes user to respective url
		$('.clickable-row').click(function(){
			window.location=this.getAttribute('data-url');
		})
		// get first letter of username
		$('.get-first-letter').each(function(){
			let user=$(this).data('user');
			$(this).text(getFirstLetters(user));
		})
		
		
	})
	
	</script>

	

